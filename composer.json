            "role": "developer"
        }
    ],
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "phpmd/phpmd": "^2.7",
        "squizlabs/php_codesniffer": "^3.5",
        "sebastian/phpcpd": "^3.0",
        "satooshi/php-coveralls": "^2.1"
    },
    "autoload": {
        "psr-0": {
